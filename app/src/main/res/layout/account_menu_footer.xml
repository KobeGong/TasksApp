<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <View style="@style/DividerStyle"/>
    <LinearLayout android:layout_gravity="center" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:id="@+id/privacy_policy" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="16dp" android:text="@string/og_privacy_policy" style="@style/FooterClickableActionItemStyle"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/bullet_separator" android:contentDescription="@null" android:importantForAccessibility="no" style="@style/FooterActionItemStyle"/>
        <TextView android:id="@+id/terms_of_service" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:text="@string/og_terms_of_service" style="@style/FooterClickableActionItemStyle"/>
    </LinearLayout>
</merge>
